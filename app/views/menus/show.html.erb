<table class='table'>
  <thead>
  <tr>
    <th scope="col">Course</th>
    <th scope="col">Name</th>
    <th scope="col">Photo</th>
    <th scope="col">Price</th>
    <th scope="col">Cart</th>
  </tr>
  </thead>
  <tbody>
    <% @menu.items.each do |i|%>
      <tr>
        <td><%= i.course %></td>
        <td><%= i.name %></td>
        <td><%= i.photo %></td>
        <td><%= i.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<div>
  <%= link_to 'Edit', edit_menu_path(@menu), class: 'btn btn-success' %>
</div>
<div>
  <%= link_to 'Back', menus_path, class: "btn btn-outline-dark back-btn" %>
</div>
